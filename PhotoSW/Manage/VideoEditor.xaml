                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Margin="0,0,1,0">
                                        <StackPanel>
                                            <Image Source="/PhotoSW;component/images/reel.png" HorizontalAlignment="Stretch" Width="86" />
                                        </StackPanel>
                                        <StackPanel>
                                            <Border Border.BorderBrush="#FF000000" Border.Background="#FF000000" Border.BorderThickness="0" Grid.Column="0" Border.Padding="0" HorizontalAlignment="Center" Grid.RowSpan="1" VerticalAlignment="Stretch" Grid.Row="0">
                                                <Grid Height="Auto" MinWidth="77" Panel.Background="#FF000000" MaxWidth="Infinity" Width="86" Margin="1,0,2,0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition RowDefinition.Height="50" />
