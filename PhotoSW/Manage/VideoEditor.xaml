                                            <Button Name="btnDropImgPlay" Style="{StaticResource ButtonStyleAlpha}" Grid.Row="1" Width="17" Height="15" Margin="0,0,3,0" ToolTip="Click to preview" HorizontalAlignment="Right" VerticalAlignment="Top" Tag="{Binding PhotoId}" UIElement.Visibility="{Binding ProcessPlayVisible}">
                                                <Image Name="imgDropPlay" Source="/PhotoSW;component/images/preview_play.png" />
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ListBox>
