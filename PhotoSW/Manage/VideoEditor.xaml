                                <ColumnDefinition ColumnDefinition.Width="510*" />
                                <ColumnDefinition ColumnDefinition.Width="150" />
                                <ColumnDefinition ColumnDefinition.Width="10" />
                            </Grid.ColumnDefinitions>
                            <StackPanel Name="VideoEditPlayerPanel" Grid.Column="0" UIElement.Visibility="Visible" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="45,5,0,0">
