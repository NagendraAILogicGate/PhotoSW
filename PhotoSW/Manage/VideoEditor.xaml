                    <ImageBrush ImageBrush.ImageSource="/PhotoSW;component/images/footer-bg.png" TileBrush.TileMode="Tile" TileBrush.ViewportUnits="Absolute" TileBrush.Viewport="0,0,42,42" />
                </Panel.Background>
            </StackPanel>
            <StackPanel Width="Auto" Margin="5,5,5,5" HorizontalAlignment="Stretch" Orientation="Horizontal">
                <StackPanel Name="LeftPanel" Width="700" Panel.Background="#FF000000" Orientation="Vertical" Margin="0,10,20,-10">
                    <Grid Name="grdMain" Width="Auto" Height="648" VerticalAlignment="Bottom" HorizontalAlignment="Stretch">
                        <Grid.RowDefinitions>
                            <RowDefinition RowDefinition.Height="120" />
                            <RowDefinition RowDefinition.Height="120" />
                            <RowDefinition RowDefinition.Height="405" />
