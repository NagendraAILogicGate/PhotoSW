                                                    <Image Source="/PhotoSW;component/images/reel.png" Width="86" Grid.Column="0" Grid.Row="1" />
                                                    <CheckBox Name="ChkVideoTemplate" UIElement.IsEnabled="False" ToggleButton.IsChecked="True" Height="Auto" Margin="0,0,0,0" HorizontalAlignment="Right" VerticalAlignment="Top" Style="{DynamicResource CheckBoxStyleimgDownloader}" Grid.RowSpan="1" Grid.Row="0" UIElement.Visibility="{Binding isVideoTemplate}" UIElement.Uid="{Binding PageNo}" />
                                                </Grid>
