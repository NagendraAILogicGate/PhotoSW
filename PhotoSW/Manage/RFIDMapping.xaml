                    <DataGrid Name="Dg_PhotographerReport" Grid.Row="3" Margin="8" RowHeight="50" CanUserAddRows="False" RowBackground="#00FFFFFF" AlternatingRowBackground="#FFFFFFFF" BorderThickness="0,0,0,3" Style="{DynamicResource DigiDataGridStyle}" RowStyle="{DynamicResource DigiDataGridRowStyle}" ColumnHeaderStyle="{DynamicResource DigiDataGridColumnHeaderStyle}" CellStyle="{DynamicResource DigiDataGridCellStyle}" Background="#FF332E32" HeadersVisibility="Column" GridLinesVisibility="None" FontSize="14.667" Foreground="#FF434343" Grid.ColumnSpan="4" AutoGenerateColumns="False" VerticalScrollBarVisibility="Auto" BorderBrush="{x:Null}">
                        <DataGrid.Columns>
                            <DataGridTextColumn FontWeight="Bold" Width="*" IsReadOnly="True" Header="Photographer" Binding="{Binding PhotographerName}" />
                            <DataGridTextColumn FontWeight="Bold" Width="*" IsReadOnly="True" Header="Location" Binding="{Binding Location}" />
                            <DataGridTextColumn FontWeight="Bold" Width="*" IsReadOnly="True" Header="Total Captured" Binding="{Binding TotalCaptured}" />
                            <DataGridTextColumn FontWeight="Bold" Width="*" IsReadOnly="True" Header="Total Associated" Binding="{Binding TotalAssociated}" />
                            <DataGridTextColumn FontWeight="Bold" Width="1.1*" IsReadOnly="True" Header="Total NonAssociated" Binding="{Binding TotalNonAssociated}" />
                            <DataGridTextColumn FontWeight="Bold" Width="*" IsReadOnly="True" Header="Last AssociatedOn" Binding="{Binding LastAssociatedOn, StringFormat={}{0:dd-MMM-yyyy HH:mm:ss}}" />
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
        </TabControl>
        <Button Name="btnBack" Grid.Row="2" FontWeight="Bold" FontSize="18.667" Content="Back" Margin="0,0,3.075,0" Style="{DynamicResource ButtonStyleRed}" Foreground="#FFFFFFFF" Height="53" HorizontalAlignment="Right" Width="127" Click="btnBack_Click" />
        <Grid Name="GrdImageDetail" Margin="0" Grid.RowSpan="3" Visibility="Collapsed">
            <Grid Margin="0">
                <Border Background="#90000000">
                    <Border BorderThickness="1" Margin="50" Background="#FFFFFFFF" CornerRadius="10">
                        <Grid Margin="0,0,5,5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="59" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="210" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Border Margin="0" Grid.ColumnSpan="2" CornerRadius="10,10,0,0" Background="#FFE5514F" />
                                    <TextBlock TextBlock.Foreground="#FFFFFFFF" Grid.Column="0" HorizontalAlignment="Left" TextBlock.FontWeight="Bold" TextBlock.FontSize="21.333" Text="Images with Details" VerticalAlignment="Center" Margin="10,0,0,0" />
                                    <StackPanel Name="stkAssociation" Orientation="Horizontal" Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,10,0" Visibility="Collapsed">
                                        <TextBox Name="txtRFIDAssociate" Width="490" Margin="8" Style="{DynamicResource DigiTextBoxStyle}" FontSize="20" HorizontalAlignment="Left" MaxLength="50" />
                                        <Button Name="btnAssociate" Grid.Row="2" FontWeight="Bold" FontSize="18.667" Content="Associate" Margin="0,0,3.075,0" Style="{DynamicResource ButtonStyleRed}" Foreground="#FFFFFFFF" Height="53" HorizontalAlignment="Left" Width="127" Click="btnAssociate_Click" />
                                    </StackPanel>
                                </Grid>
                                <Button Name="btnImageClose" Style="{StaticResource ButtonStyleAlpha2}" HorizontalAlignment="Right" VerticalAlignment="Top" Width="97" Margin="0,-82.345,-60,0" Height="141.346">
                                    <Image Source="../images/close-btn.png" />
                                </Button>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <ListBox Name="lstImages" ItemsPanel="{DynamicResource DigiItemsPanelTemplateProduct}" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Padding="0" Margin="15,48,15,20" HorizontalContentAlignment="Center" Template="{DynamicResource ListBoxControlTemplategeneral}">
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid Height="Auto" MinWidth="77" Margin="3,15,3,3" MaxWidth="Infinity" Width="166">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="22" />
                                                    <RowDefinition Height="100" />
                                                    <RowDefinition Height="22" />
                                                </Grid.RowDefinitions>
                                                <Rectangle Fill="#FFFFFFFF" Grid.Row="0" Grid.RowSpan="3" Stroke="#FF000000" StrokeThickness="1" HorizontalAlignment="Stretch" />
                                                <TextBlock Name="image_Date" Margin="5,0,0,0" Grid.Column="0" TextWrapping="Wrap" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextBlock.FontSize="12" TextBlock.FontWeight="Normal" Width="Auto" Height="Auto" Grid.Row="0" TextBlock.TextAlignment="Center" TextBlock.Foreground="#FF000000" Text="{Binding DG_Photos_CreatedOn}" />
                                                <Image Name="image" Margin="0" Stretch="Uniform" Grid.RowSpan="1" VerticalAlignment="Stretch" Grid.Row="1" Source="{Binding FileName}" />
                                                <CheckBox Name="chkSelectedimages"  Content="CheckBox" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Style="{DynamicResource CheckBoxStyleimgDownloader}" Width="Auto" Height="Auto" Grid.RowSpan="2" Margin="0,0,4,2" IsChecked="{Binding IsChecked}" IsEnabled="{Binding IsEnabled}" />
                                                <TextBlock Name="image_RFID" Grid.Column="0" TextWrapping="Wrap" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextBlock.FontSize="12" TextBlock.FontWeight="Normal" Width="Auto" Height="Auto" Grid.Row="2" TextBlock.TextAlignment="Center" TextBlock.Foreground="#FF000000" Text="{Binding PhotoRFID}" />
                                            </Grid>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                            </Grid>
                        </Grid>
                    </Border>
                </Border>
            </Grid>
        </Grid>
    </Grid>
</Window>
