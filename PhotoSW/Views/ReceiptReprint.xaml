                    <DataTemplate x:Name="mytemplate">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="Auto" MinHeight="15.96" />
                                <RowDefinition Height="Auto" MinHeight="15.96" />
                            </Grid.RowDefinitions>
                            <Border BorderThickness="2" BorderBrush="#FF000000">
