                                    <Image HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="Auto" Source="{Binding FilePath, Converter={StaticResource Imageconverter}}" Visibility="{Binding ImageVisibility}" />
                                    <TextBlock TextBlock.FontSize="10" HorizontalAlignment="Stretch" VerticalAlignment="Center" Width="Auto" Text="{Binding PhotoName}" Visibility="{Binding OtherVisibility}" />
                                </Grid>
                            </Border>
                            <TextBlock Grid.Row="1" Text="{Binding PhotoName}" />
                            <TextBlock Grid.Row="2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Text="{Binding AmountRefunded, StringFormat=N2}" />
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemsControl.ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <WrapPanel />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
