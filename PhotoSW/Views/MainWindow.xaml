<Window x:Class="PhotoSW.Views.MainWindow" Title="PhotoSWMain" 
        UseLayoutRounding="True" Height="Auto" Width="Auto" 
        Background="#606060"
        Unloaded="Window_Unloaded" 
        Closed="Window_Closed" 
        MouseDown="Window_MouseDown"        
        MouseUp="Window_MouseUp"     
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:fa="http://schemas.fontawesome.io/icons/"
        xmlns:dmb="clr-namespace:PhotoSW.PSControls"
        xmlns:utbc="clr-namespace:PhotoSW.Common;assembly=FrameworkHelper"
        xmlns:jr="clr-namespace:PhotoSW.PSControls"
        xmlns:jas="clr-namespace:PhotoSW.DragImage" 
        xmlns:local="clr-namespace:PhotoSW.ViewModels"         
        xmlns:localGIF="clr-namespace:PhotoSW.Views"
