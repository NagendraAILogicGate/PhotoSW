                                    <ColumnDefinition Width="0.662*" />
                                    <ColumnDefinition Width="Auto" MinWidth="11.3176666666667" />
                                    <ColumnDefinition Width="0.338*" />
                                    <ColumnDefinition Width="55" />
                                </Grid.ColumnDefinitions>
                                <Button Name="btnPrint" Margin="0,5,3,0" Style="{StaticResource ButtonStyleAlpha}" ToolTip="Add to print group" HorizontalAlignment="Right" Grid.Column="4" Height="45" 
                                        Tag="{Binding Name}" CommandParameter="{Binding PhotoId}" Click="btnPrint_Click">
                                    <Image Width="36" Height="34" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Source="{Binding PrintGroup}" />
                                </Button>
                                <Image Name="imgProcessed" Source="/PhotoSW;component/images/processed.png" Grid.Column="2" Width="22" Height="22" Margin="0,4,0,0" ToolTip="Processed Video" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding ProcessedVisible}"  />
