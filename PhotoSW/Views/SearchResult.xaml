                                        Click="btnAddToGroup_Click">
                                    <Image Name="ImgAddToGroup" Width="36" Height="34" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Source="{Binding BmpImageGroup}" />
                                </Button>
                            </Grid>
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemsControl.ItemsPanel">
