                                </Button>
                            </Grid>
                            <Grid Name="Printbtns" Margin="15,2,15,0" Grid.Row="1" VerticalAlignment="Top" Height="49.225" HorizontalAlignment="Stretch" Width="Auto" Visibility="{Binding IsLocked}">
                                <Grid.ColumnDefinitions>
